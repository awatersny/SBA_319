<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Callouts</title>
</head>
<body>
  <div id="container">
    <% mapModes.forEach(mapMode => { %>
      <div class="info-box">
        <a href="/callouts/<%= mapMode._id %>">
          <h2><%= mapMode.stage %> <%= mapMode.mode %> Callouts </h2>
        </a>
        <img 
          src="<%= mapMode.callout %>"
          alt=" <%= mapMode.stage %> <%= mapMode.mode %> Callouts "
          width="1000px"
        >
      </div>
    <% }); %>
  </div>
</body>
</html>